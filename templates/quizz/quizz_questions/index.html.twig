{% extends 'base.html.twig' %}

{% block title %}QuizzQuestions index{% endblock %}

{% block body %}
    <h1>QuizzQuestions index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Question</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for quizz_question in quizz_questions %}
            <tr>
                <td>{{ quizz_question.id }}</td>
                <td>{{ quizz_question.question }}</td>
                <td>
                    <a href="{{ path('app_quizz_quizz_questions_show', {'id': quizz_question.id}) }}">show</a>
                    <a href="{{ path('app_quizz_quizz_questions_edit', {'id': quizz_question.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_quizz_quizz_questions_new') }}">Create new</a>
{% endblock %}
